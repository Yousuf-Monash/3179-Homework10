{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 450,
  "title": "Earthquakes of magnitude 4 or large between 4 Oct 2020 and 10 Oct 2020",
  "projection": {"type": "equalEarth"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/Yousuf-Monash/3179-Homework-Week-9/main/js/ne_110m_admin_0_countries.json",
        "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://github.com/Yousuf-Monash/3179-Homework-Week-9/blob/main/data/global-data-on-sustainable-energy.csv"
      },
      "mark": {"type": "circle", "tooltip": {"content": "data"}},
      "encoding": {
        "longitude": {"field": "Longitude", "type": "quantitative"},
        "latitude": {"field": "Latitude", "type": "quantitative"},
        "size": {
          "field": "Renewable-electricity-generating-capacity-per-capita",
          "type": "quantitative",
          "title": "Magnitude (Richter scale)"
        }
      }
    }
  ]
}